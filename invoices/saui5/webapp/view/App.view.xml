<mvc:View   controllerName="logaligroup.saui5.controller.App"
            xmlns="sap.m" 
            xmlns:mvc="sap.ui.core.mvc"
            displayBlock="true">
        <Shell>
            <App>
                <pages>
                    <Page title="{i18n>appTitle}" >
                    <headerContent>
                        <Button id="helloDialogHeader" 
                                icon="sap-icon://action" 
                                press=".onOpenDialogHeader" 
                                class='sapUiSmallMarginEnd'/>
                    </headerContent>
                    <content>
                        <mvc:XMLView viewName="logaligroup.saui5.view.helloPanel"/>
                        <mvc:XMLView viewName="logaligroup.saui5.view.InvoicesList"/>
                    </content>
                    </Page>
                </pages>
            </App>
        </Shell>
</mvc:View>