<mvc:View controllerName="logaligroup.saui5.controller.Details"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:logali="logaligroup.saui5.control" displayBlock="true">
    <Page title="{i18n>detailsPagelTitle}" showNavButton="true" navButtonPress=".onNavBack">
        <ObjectHeader title="Detalle Factura">
            <attributes>
                <ObjectAttribute text="{Northwind>ShipName}"/>
                <ObjectAttribute text="{Northwind>ShipAddress}"/>
                <ObjectAttribute text="{Northwind>ShipCity}"/>
                <ObjectAttribute text="{Northwind>ShipRegion}"/>
                <ObjectAttribute text="{Northwind>ShipPostalCode}"/>
                <ObjectAttribute text="{Northwind>ShipCountry}"/>
                <ObjectAttribute text="{Northwind>CustomerID}"/>
                <ObjectAttribute text="{Northwind>CustomerName}"/>
            </attributes>
        </ObjectHeader>
        <logali:ProductRating id="rating" class="sapUiSmallMarginBeginEnd" change=".onRatingChange"/>
    </Page>
</mvc:View>